<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lizp.springboot.mapper.main.MainStudentDao">
	<resultMap id="BaseResultMap" type="com.lizp.springboot.entity.main.Student">
		<id column="id" jdbcType="BIGINT" property="id" />
		<result column="name" jdbcType="VARCHAR" property="name" />
		<result column="age" jdbcType="INTEGER" property="age" />
	</resultMap>

	<insert id="insert" parameterType="com.lizp.springboot.entity.main.Student">
		insert into student(name, age)
		values (#{name,jdbcType=VARCHAR}, #{age,jdbcType=INTEGER})
	</insert>
</mapper>